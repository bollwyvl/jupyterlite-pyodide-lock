{
    "LiteBuildConfig": {
        "cache_dir": "../build/.cache/lite",
        "output_dir": "../build/docs-app",
        "contents": ["."]
    },
    "PyodideLockAddon": {
        "enabled": true,
        "specs": ["ipywidgets >=8.1,<8.2"],
        "extra_preload_packages": ["ipywidgets", "piplite"]
    },
    "BrowserLocker": {
        "private_mode": true,
        "headless": true,
        "browser": "firefox",
        "profile": "../build/.cache/lite/.firefox-profile"
    }
}
